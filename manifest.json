{
  "applications": {
    "gecko": {
      "id": "{b81e65cd-2988-4e27-8c9b-088c7c58101a}"
    }
  },
  "manifest_version": 2,
  "name": "Google Cloud Print",
  "description": "Print anything on Google Cloud Print from Firefox",
  "version": "0.0.2",
  "author": "Giovanni T. Parra",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "icon.png",
    "default_title": "Print on Google Cloud Print"
  },
  "permissions": [
    "activeTab"
  ]
}
