{
  "applications": {
    "gecko": {
      "id": "{b81e65cd-2988-4e27-8c9b-088c7c58101a}"
    }
  },
  "manifest_version": 2,
  "name": "Google Cloud Print",
  "description": "Print any page on Google Cloud Print directly from Firefox",
  "version": "0.0.3",
  "author": "Giovanni T. Parra",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Print this page on Google Cloud Print"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "https://mercury.postlight.com/parser"
  ]
}
